<!DOCTYPE node PUBLIC "-//freedesktop//DTD D-BUS Object Introspection 1.0//EN"
"http://www.freedesktop.org/standards/dbus/1.0/introspect.dtd">
<node>
  <interface name="org.freedesktop.ConsoleKit.Manager">
    <method name="GetSeats">
      <arg name="seats" direction="out" type="ao"/>
    </method>
    <method name="GetSessions">
      <arg name="sessions" direction="out" type="ao"/>
    </method>
    <method name="GetSessionForCookie">
      <arg name="cookie" direction="in" type="s"/>
      <arg name="ssid" direction="out" type="o"/>
    </method>
    <method name="GetSessionForUnixProcess">
      <arg name="pid" direction="in" type="u"/>
      <arg name="ssid" direction="out" type="o"/>
    </method>
    <method name="GetCurrentSession">
      <arg name="ssid" direction="out" type="o"/>
    </method>
    <method name="GetSessionsForUnixUser">
      <arg name="uid" direction="in" type="u"/>
      <arg name="sessions" direction="out" type="ao"/>
    </method>
    <method name="GetSessionsForUser">
      <arg name="uid" direction="in" type="u"/>
      <arg name="sessions" direction="out" type="ao"/>
    </method>
    <method name="OpenSession">
      <arg name="cookie" direction="out" type="s"/>
    </method>
    <method name="OpenSessionWithParameters">
      <arg name="parameters" direction="in" type="a(sv)"/>
      <arg name="cookie" direction="out" type="s"/>
    </method>
    <method name="CloseSession">
      <arg name="cookie" direction="in" type="s"/>
      <arg name="result" direction="out" type="b"/>
    </method>
    <method name="CanRestart">
      <arg name="can_restart" direction="out" type="s"/>
    </method>
    <method name="CanStop">
      <arg name="can_stop" direction="out" type="s"/>
    </method>
    <method name="Restart"/>
    <method name="Stop"/>
    <method name="Suspend">
      <arg name="policykit_interactivity" direction="in" type="b"/>
    </method>
    <method name="Hibernate">
      <arg name="policykit_interactivity" direction="in" type="b"/>
    </method>
    <method name="CanSuspend">
      <arg name="can_suspend" direction="out" type="s"/>
    </method>
    <method name="CanHibernate">
      <arg name="can_hibernate" direction="out" type="s"/>
    </method>
    <signal name="SeatAdded">
      <arg name="sid" type="o"/>
    </signal>
    <signal name="SeatRemoved">
      <arg name="sid" type="o"/>
    </signal>
    <signal name="SystemIdleHintChanged">
      <arg name="hint" type="b"/>
    </signal>
  </interface>
</node>
