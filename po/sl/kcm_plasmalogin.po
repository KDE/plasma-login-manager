msgstr "Privzeti uporabnik:"

#: ui/main.qml:235
#, kde-format
msgctxt "@option:radio"
msgid "Last logged-in user"
msgstr "Zadnje prijavljeni uporabnik"

#: ui/main.qml:306
#, kde-format
msgctxt "@label"
msgid "Default session:"
msgstr "Privzeta seja:"

#: ui/main.qml:312
#, kde-format
msgctxt "@option:radio"
msgid "Last logged-in session"
msgstr "Zadnja prijavljena seja"

